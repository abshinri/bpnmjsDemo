{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.d.ts",
    "types/**/*.d.ts"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "types/*": ["types/*"]
    },
    "types": ["element-plus/global", "node"]
  },

  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
